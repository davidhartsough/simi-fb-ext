{
  "manifest_version": 3,
  "name": "SiMi FB",
  "version": "1.0",
  "description": "Simplistic Minimalistic Facebook",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "js": ["simi-fb-home.js"],
      "matches": ["https://www.facebook.com/"]
    },
    {
      "css": ["simi-fb.css"],
      "js": ["simi-fb.js"],
      "matches": ["https://www.facebook.com/*"]
    }
  ]
}
